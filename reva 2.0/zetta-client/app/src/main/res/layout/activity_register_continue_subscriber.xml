<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".browse.Registration"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <EditText
        android:id="@+id/txt_subscribePass"
        android:layout_width="262dp"
        android:layout_height="45dp"
        android:ems="10"
        android:textSize="16sp"
        android:inputType="none"
        android:labelFor="@+id/input_confirmPassReg"
        android:text="Password to Subscribe to Patient:"
        android:textColor="@android:color/background_dark"
        tools:ignore="HardcodedText,LabelFor,RtlHardcoded"
        tools:text="Password to Subscribe to Patient:"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/input_whoSub"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <EditText
        android:id="@+id/input_subToPatientPass"
        android:layout_width="262dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="************"
        android:inputType="textPassword"
        tools:ignore="HardcodedText,RtlHardcoded"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginTop="0dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/txt_subscribePass"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/btn_subRegDone"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/txt_whoSub"
        android:layout_width="262dp"
        android:layout_height="80dp"
        android:layout_marginEnd="33dp"
        android:layout_marginStart="31dp"
        android:textSize="16sp"
        android:ems="10"
        android:inputType="none"
        android:labelFor="@+id/input_whoSub"
        android:maxLines="3"
        android:text="To Whom Are You Subscribing? Please Type in Either Their E-Mail or User Name:"
        android:textColor="@android:color/background_dark"
        android:textColorHighlight="@android:color/background_dark"
        android:textIsSelectable="false"
        tools:ignore="HardcodedText,LabelFor,RtlHardcoded"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:text="To Whom Are You Subscribing? Please Type in Either Their E-Mail or User Name:"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/radioGroupReg4"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/txt_register"
        style="@style/Widget.AppCompat.Button.Colored"
        android:background="@android:color/holo_blue_dark"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:ems="10"
        android:inputType="none"

        android:maxLines="1"
        android:text="Registration"
        android:gravity="center"
        android:textIsSelectable="false"
        tools:ignore="HardcodedText,LabelFor"
        tools:text="Registration"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="0dp"
        app:layout_constraintHorizontal_bias="0.0" />

    <EditText
        android:id="@+id/input_whoSub"
        android:layout_width="262dp"
        android:layout_height="45dp"
        android:layout_marginStart="31dp"
        android:ems="10"
        android:hint="Charles Sanders"
        android:inputType="textWebEmailAddress"
        android:selectAllOnFocus="false"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginEnd="24dp"
        tools:layout_constraintLeft_creator="1"
        tools:ignore="HardcodedText"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/txt_whoSub"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <EditText
        android:id="@+id/txt_Title"
        android:layout_width="262dp"
        android:layout_height="45dp"
        android:layout_marginStart="31dp"
        android:textSize="15sp"
        android:ems="10"
        android:inputType="none"
        android:labelFor="@+id/input_passwordReg"
        android:text="What is Your Relation to the Patient?"
        android:textColor="@android:color/background_dark"
        tools:ignore="HardcodedText,LabelFor,RtlHardcoded"
        tools:text="What is Your Relation to the Patient?"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/txt_register" />

    <Button
        android:id="@+id/btn_subRegDone"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="262dp"
        android:layout_height="45dp"
        android:layout_marginStart="31dp"
        android:background="@android:color/holo_blue_dark"
        android:text="Register"
        tools:ignore="HardcodedText,RtlHardcoded"
        tools:text="Register"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="44dp" />


    <RadioGroup
        android:id="@+id/radioGroupReg5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="0dp"
        android:orientation="horizontal"
        app:layout_constraintHorizontal_bias="0.346"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_Title"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1">

        <RadioButton
            android:id="@+id/rad_family"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:text="Family"
            tools:ignore="HardcodedText"
            tools:layout_editor_absoluteX="31dp"
            tools:layout_editor_absoluteY="135dp"
            tools:text="Family" />

        <RadioButton
            android:id="@+id/rad_caretaker"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:text="Caretaker"
            tools:ignore="HardcodedText"
            tools:layout_editor_absoluteX="100dp"
            tools:layout_editor_absoluteY="134dp"
            tools:text="Caretaker" />
    </RadioGroup>

    <RadioGroup
        android:id="@+id/radioGroupReg4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="0dp"
        android:orientation="horizontal"
        app:layout_constraintHorizontal_bias="0.306"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroupReg5"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1">

        <RadioButton
            android:id="@+id/rad_researcher"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Researcher"
            tools:ignore="HardcodedText"
            tools:layout_editor_absoluteX="49dp"
            tools:layout_editor_absoluteY="167dp"
            tools:text="Researcher" />

        <RadioButton
            android:id="@+id/rad_doctor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:text="Doctor"
            tools:ignore="HardcodedText"
            tools:layout_editor_absoluteX="168dp"
            tools:layout_editor_absoluteY="167dp"
            tools:text="Doctor" />
    </RadioGroup>


</android.support.constraint.ConstraintLayout>
