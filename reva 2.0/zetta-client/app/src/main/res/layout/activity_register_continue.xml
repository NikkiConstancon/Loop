<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".browse.Registration"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <EditText
        android:id="@+id/txt_userName"
        android:textSize="12sp"
        android:layout_width="262dp"
        android:layout_height="35dp"
        android:ems="10"
        android:inputType="none"
        android:labelFor="@id/rad_patYes"
        android:text="Please Enter a User Name:"
        android:textColor="@android:color/background_dark"
        tools:ignore="HardcodedText,LabelFor,RtlHardcoded"
        tools:text="Please Enter a User Name:"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="31dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="-8dp"
        app:layout_constraintBottom_toTopOf="@+id/input_userName" />

    <EditText
        android:id="@+id/input_userName"
        android:layout_width="262dp"
        android:layout_height="35dp"
        android:textSize="12sp"
        android:ems="10"
        android:hint="Carl Sanders"
        android:textColorHint="@color/textColorTertiary"
        android:inputType="textPassword"
        tools:ignore="HardcodedText,RtlHardcoded"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        tools:layout_editor_absoluteY="241dp" />

    <EditText
        android:id="@+id/txt_addr"
        android:layout_width="262dp"
        android:layout_height="35dp"
        android:textSize="12sp"
        android:ems="10"
        android:inputType="none"
        android:labelFor="@+id/input_addr"
        android:maxLines="1"
        android:text="What Is Your Address?"
        android:textColor="@android:color/background_dark"
        android:textColorHighlight="@android:color/background_dark"
        android:textIsSelectable="false"
        tools:ignore="HardcodedText,LabelFor,RtlHardcoded"
        tools:text="What Is Your Address?"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginBottom="-8dp"
        app:layout_constraintBottom_toTopOf="@+id/input_addr" />

    <TextView
        android:id="@+id/txt_register"
        style="@style/Widget.AppCompat.Button.Colored"
        android:background="@android:color/holo_blue_dark"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:ems="10"
        android:inputType="none"

        android:maxLines="1"
        android:text="Registration"
        android:gravity="center"
        android:textIsSelectable="false"
        tools:ignore="HardcodedText,LabelFor"
        tools:text="Registration"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginBottom="80dp"
        app:layout_constraintBottom_toTopOf="@+id/txt_addr" />

    <EditText
        android:id="@+id/input_addr"
        android:layout_width="262dp"
        android:layout_height="35dp"
        android:ems="10"
        android:textSize="12sp"
        android:hint="26 Anderson Street, Hatfield"
        android:textColorHint="@color/textColorTertiary"
        android:inputType="textWebEmailAddress"
        android:selectAllOnFocus="false"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:ignore="HardcodedText"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginBottom="11dp"
        app:layout_constraintBottom_toTopOf="@+id/txt_userName" />


    <Button
        android:id="@+id/btn_patRegCont"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="262dp"
        android:layout_height="45dp"
        android:background="@android:color/holo_blue_dark"
        android:text="Continue"
        tools:ignore="HardcodedText,RtlHardcoded"
        tools:text="Continue"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="31dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="36dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

</android.support.constraint.ConstraintLayout>
